// 90-100: Grade A
// 80-89: Grade B
// 70-79: Grade C
// 60-69: Grade D
// 0-59: Grade F

#include <stdio.h>
#include <stdlib.h>

int main(){
    
    int score;
    printf("Enter You Score:\n");
    scanf("%d", &score);
    
    if(score>=90 && score <=100){
        printf("Grade A");
        
    }
    
    else if(score>=80 && score <=89){
        printf("Grade B");
    }
     else if(score>=70 && score <=79){
        printf("Grade C");
    }
    
      else if(score>=60 && score <=69){
        printf("Grade D");
    }
    
    else{
        printf("Grade F");
    }
    

    
    return 0;
}